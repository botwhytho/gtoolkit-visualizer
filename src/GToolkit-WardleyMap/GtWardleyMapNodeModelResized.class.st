Class {
	#name : #GtWardleyMapNodeModelResized,
	#superclass : #GtWardleyMapNodeModelAnnouncement,
	#instVars : [
		'nodeModel'
	],
	#category : #'GToolkit-WardleyMap-Events'
}

{ #category : #accessing }
GtWardleyMapNodeModelResized >> nodeModel [
